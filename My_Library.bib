@article{edwardsDisabilityBodiesBearers2003,
	title = {Disability and Bodies as Bearers of Value},
	volume = {37},
	issn = {0038-0385, 1469-8684},
	url = {http://journals.sagepub.com/doi/10.1177/0038038503037002002},
	doi = {10.1177/0038038503037002002},
	abstract = {This article considers the relevance of Pierre Bourdieu’s conceptions of the body to the development of disability theory. We begin by discussing the limitations of reductive conceptions of disability. In so doing, we consider how far Bourdieu’s (1990) concept of habitus offers a way of bringing an analysis of the body to bear upon an understanding of the social inequalities which are core to the lives of disabled people. Through focus groups with disabled people, the article explores aspects of disabled people’s corporeal identities, feelings and (embodied) encounters in a range of social settings. The research shows that disabled people’s lives are connected to different ‘valuations’ attributed to corporeal forms, and to systems of signiﬁcation and representation, which underpin them. We conclude by reafﬁrming the need to consider Bourdieu’s ideas in helping in the development of disability theory.},
	pages = {239--256},
	number = {2},
	journaltitle = {Sociology},
	shortjournal = {Sociology},
