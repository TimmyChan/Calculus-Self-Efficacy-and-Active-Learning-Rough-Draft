@inproceedings{alozieChallengesDesigningInstructionally18,
	location = {New York, {NY}},
	title = {Challenges in Designing Instructionally Supportive Science Assessments Using Culturally Relevant Principles for Diverse Students},
	abstract = {The goal of this paper is to highlight tensions and challenges that arise in designing science assessment tasks that promote cultural relevance for historically underrepresented students (e.g., low socioeconomic status ({SES}) and ethnically and culturally underrepresented students). We present our process for developing equitable and instructionally supportive assessment tasks for multicultural students, resulting assessment tasks, and challenges associated with attending to
cultural relevance (Ladson-Billings, 1995; 2014) in assessment design. We ask the question: “How can we achieve equity and fairness in the design of assessment tasks that incorporate tenets of culturally relevant pedagogy ({CRP}) without introducing construct irrelevant variance?” We present an equity and fairness assessment design framework and explore the infusion of {CRP} in our assessment design process to determine the extent to which culturally relevant pedagogy can be applied. This work is in response to the adoption of the Next Generation Science Standards ({NGSS}), where teachers are faced with how to assess students from diverse backgrounds in science concepts, themes, and practices. Our design work is being carried out in the context of a multi-institutional collaborative, where we recognized a need for a clear and efficient approach to designing assessment tasks that can inform instruction in increasingly
diverse classrooms.},
	eventtitle = {annual meeting of the American Educational Research Association},
	booktitle = {Bringing equity to science education through curriculum, assessment, and community-based experimental learning},
	publisher = {{AERA}},
	author = {Alozie, Nonye and Madden, Krystal Nacole Alberta and Zaidi, Sania Zahra and Pennock, Phyllis Haugabook and Harris, Christopher J.},
	date = {2018-04-13}
}

@article{altinpullukDeterminingTrendsUsing2019,
	title = {Determining the trends of using augmented reality in education between 2006-2016},
	volume = {24},
	issn = {1360-2357, 1573-7608},
	url = {http://link.springer.com/10.1007/s10639-018-9806-3},
	doi = {10.1007/s10639-018-9806-3},
	abstract = {This study aims to examine 58 articles, published in 8 educational technology journals under the Social Science Citation Index ({SSCI}) between 2006 and 2016, containing Baugmented reality{\textasciicircum} in their title and, thereby, to determine the trends of using augmented reality ({AR}) in education by the content analysis technique. In this period of time, the total number of academic publications containing Baugmented reality{\textasciicircum} in the Web of Science database, in all indices, was determined as 3540, and among these publications, 58 articles with {SSCI} index were analyzed within the scope of the research. It analyzes the change of articles related to {AR} by years; the usage of methods and data collection tools; education field, educational advantages, the type of applications used, technologies, assets used, physical environments in which researches were carried out, sensory types that applications address to, countries and continents in which the articles were published, and the keywords in these articles. Findings obtained were visualized with tables and graphics. According to the results obtained from the findings, there has been a sudden increase in the number of publications since 2013 and it reached the highest level in 2016, the most commonly used method is the mixed method and the data collection tool is questionnaire, most of the studies are carried out in Science, tablets and smartphones are most used, {AR} applications address to visual sense mostly and such articles are published mostly in the Asian continent. It has been determined that the most positive effect of {AR} on education is for academic success and learning motivation. It has been understood that the {AR} is closely related to disciplines such as human-computer interaction, robotics and remote laboratory systems. In addition, some suggestions have been made to researchers and developers based on these research findings.},
	pages = {1089--1114},
	number = {2},
	journaltitle = {Education and Information Technologies},
	shortjournal = {Educ Inf Technol},
	author = {Altinpulluk, Hakan},
	urldate = {2019-11-02},
	date = {2019-03},
	langid = {english}
}

